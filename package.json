{
  "private": true,
  "name": "watermark-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "confold align && husky install",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --ignore-path .gitignore --check **/*.{ts,js,json,yaml,yml,svelte,html,css} && eslint .",
    "format": "prettier --ignore-path .gitignore --write **/*.{ts,js,json,yaml,yml,svelte,html,css}",
    "changeset": "changeset",
    "gen-pwa-assets": "pwa-assets-generator --preset minimal static/icon.1024.png"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@iconify/svelte": "^3.1.4",
    "@playwright/test": "^1.36.0",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@tailwindcss/typography": "^0.5.9",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vite-pwa/assets-generator": "^0.0.3",
    "@vite-pwa/sveltekit": "^0.2.5",
    "autoprefixer": "^10.4.14",
    "confold": "^0.1.2",
    "daisyui": "^3.2.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "jszip": "^3.10.1",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.6",
    "svelte-i18n": "^3.7.0",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.34.0",
    "workbox-window": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,js,json,yaml,yml,svelte,html,css}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.6.7"
}
